{
    "name": "wp-starter/mail",
    "description": "The WpStarter Mail package.",
    "license": "MIT",
    "homepage": "https://wpstarter.dev",
    "support": {
        "issues": "https://github.com/wpstarter/framework/issues",
        "source": "https://github.com/wpstarter/framework"
    },
    "authors": [
        {
            "name": "As247",
            "email": "as247@wpstarter.dev"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "wp-starter/collections": "^1.0",
        "wp-starter/container": "^1.0",
        "wp-starter/contracts": "^1.0",
        "wp-starter/macroable": "^1.0",
        "wp-starter/support": "^1.0",
        "league/commonmark": "^1.3|^2.0.2",
        "psr/log": "^1.0|^2.0",
        "swiftmailer/swiftmailer": "^6.3",
        "tijsverkoyen/css-to-inline-styles": "^2.2.2"
    },
    "autoload": {
        "psr-4": {
            "WpStarter\\Mail\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "aws/aws-sdk-php": "Required to use the SES mail driver (^3.198.1).",
        "guzzlehttp/guzzle": "Required to use the Mailgun mail driver (^6.5.5|^7.0.1).",
        "wildbit/swiftmailer-postmark": "Required to use Postmark mail driver (^3.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
