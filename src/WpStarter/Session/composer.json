{
    "name": "wpstarter/session",
    "description": "The WpStarter Session package.",
    "license": "MIT",
    "homepage": "https://wpstarter.dev",
    "support": {
        "issues": "https://github.com/wpstarter/framework/issues",
        "source": "https://github.com/wpstarter/framework"
    },
    "authors": [
        {
            "name": "As247",
            "email": "as247@wpstarter.dev"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "wpstarter/collections": "^1.0",
        "wpstarter/contracts": "^1.0",
        "wpstarter/filesystem": "^1.0",
        "wpstarter/support": "^1.0",
        "symfony/finder": "^5.4",
        "symfony/http-foundation": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "WpStarter\\Session\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "wpstarter/console": "Required to use the session:table command (^8.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
