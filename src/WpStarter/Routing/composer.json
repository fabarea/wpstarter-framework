{
    "name": "wp-starter/routing",
    "description": "The WpStarter Routing package.",
    "license": "MIT",
    "homepage": "https://wpstarter.dev",
    "support": {
        "issues": "https://github.com/wpstarter/framework/issues",
        "source": "https://github.com/wpstarter/framework"
    },
    "authors": [
        {
            "name": "As247",
            "email": "as247@wpstarter.dev"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "wp-starter/collections": "^1.0",
        "wp-starter/container": "^1.0",
        "wp-starter/contracts": "^1.0",
        "wp-starter/http": "^1.0",
        "wp-starter/macroable": "^1.0",
        "wp-starter/pipeline": "^1.0",
        "wp-starter/session": "^1.0",
        "wp-starter/support": "^1.0",
        "symfony/http-foundation": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/routing": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "WpStarter\\Routing\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "wp-starter/console": "Required to use the make commands (^8.0).",
        "nyholm/psr7": "Required to use PSR-7 bridging features (^1.2).",
        "symfony/psr-http-message-bridge": "Required to use PSR-7 bridging features (^2.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
