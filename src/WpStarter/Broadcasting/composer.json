{
    "name": "wp-starter/broadcasting",
    "description": "The WpStarter Broadcasting package.",
    "license": "MIT",
    "homepage": "https://wpstarter.dev",
    "support": {
        "issues": "https://github.com/wpstarter/framework/issues",
        "source": "https://github.com/wpstarter/framework"
    },
    "authors": [
        {
            "name": "As247",
            "email": "as247@wpstarter.dev"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "psr/log": "^1.0|^2.0",
        "wp-starter/bus": "^1.0",
        "wp-starter/collections": "^1.0",
        "wp-starter/contracts": "^1.0",
        "wp-starter/queue": "^1.0",
        "wp-starter/support": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WpStarter\\Broadcasting\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "ably/ably-php": "Required to use the Ably broadcast driver (^1.0).",
        "pusher/pusher-php-server": "Required to use the Pusher broadcast driver (^4.0|^5.0|^6.0|^7.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
