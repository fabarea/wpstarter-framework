{
    "name": "wp-starter/auth",
    "description": "The WpStarter Auth package.",
    "license": "MIT",
    "homepage": "https://wpstarter.dev",
    "support": {
        "issues": "https://github.com/wpstarter/framework/issues",
        "source": "https://github.com/wpstarter/framework"
    },
    "authors": [
        {
            "name": "As247",
            "email": "as247@wpstarter.dev"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "wpstarter/collections": "^1.0",
        "wpstarter/contracts": "^1.0",
        "wpstarter/http": "^1.0",
        "wpstarter/macroable": "^1.0",
        "wpstarter/queue": "^1.0",
        "wpstarter/support": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WpStarter\\Auth\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "wpstarter/console": "Required to use the auth:clear-resets command (^8.0).",
        "wpstarter/queue": "Required to fire login / logout events (^8.0).",
        "wpstarter/session": "Required to use the session based guard (^8.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
