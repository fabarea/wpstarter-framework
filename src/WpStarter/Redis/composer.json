{
    "name": "wp-starter/redis",
    "description": "The WpStarter Redis package.",
    "license": "MIT",
    "homepage": "https://wpstarter.dev",
    "support": {
        "issues": "https://github.com/wpstarter/framework/issues",
        "source": "https://github.com/wpstarter/framework"
    },
    "authors": [
        {
            "name": "As247",
            "email": "as247@wpstarter.dev"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "wpstarter/collections": "^1.0",
        "wpstarter/contracts": "^1.0",
        "wpstarter/macroable": "^1.0",
        "wpstarter/support": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WpStarter\\Redis\\": ""
        }
    },
    "suggest": {
        "ext-redis": "Required to use the phpredis connector (^4.0|^5.0).",
        "predis/predis": "Required to use the predis connector (^1.1.9)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
